{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "balar <marefa9cb8@gmail.com>",
  "engines": {
    "node": "22.x.x",
    "npm": "10.x.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:types",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check --experimental-cli .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier --write --experimental-cli .",
    "test": "vitest run",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@prettier/plugin-oxc": "0.0.4",
    "@tailwindcss/vite": "4.1.11",
    "@types/node": "22.16.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react-swc": "3.11.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "typescript-eslint": "8.37.0",
    "vite": "7.0.5",
    "vitest": "3.2.4"
  }
